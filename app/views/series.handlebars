<div class="row sponsor-main">
  <div class="medium-8 small-12 columns">
    {{#if finalResults}}
      <h3>Overall Series Results: <a href="{{ finalResults }}" target="_blank">Click Here</a></h3>
    {{/if}}
    {{#if results}}
      <h4>Current Point Standings: <a href="/uploads/{{ results }}" target="_blank">Click Here</a></h3>
    {{/if}}
    {{{ seriesInfo }}}
  </div>
  <div class="medium-3 small-12 columns right-sponsors">
    <h3>Sponsors</h3>
    <hr/>
    {{#nsponsors sponsors 3}}
    <a href="{{ url }}" target="_blank" class="side-sponsor">
      <img class="sponsor-thumbnail" src="/uploads/{{ logoUrl }}"/>
    </a>
    {{/nsponsors}}
  </div>
</div>
